{
  "$schema": "https://json.schemastore.org/swcrc",
  "env": {
    "targets": {
        "node": 18
    }
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": true
    },
    "target": "es2017"
  },
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": "inline"
}